{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(local);Database=Binebase.Exchange.Gateway;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "RedisCacheClient.Configuration": {
    "ConnectionString": "127.0.0.1:6379"
  },
  "CryptoService.Configuration": {
    "Address": "https://localhost:5002"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ExchangeRateService.Configuration": {
    "SupportedPairs": [ "BTC/EURB", "ETH/EURB", "BINE/EURB" ],
    "ExchangeExcludePairs": [ "BINE/EURB" ],
    "BineRefreshRate": "1.00:0:00",
    "BineBaseValue": 0.1,
    "BineRange": [ 0.06, 0.12 ]
  }
}
