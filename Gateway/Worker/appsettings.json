{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Warning"
            }
        }
    },
    "Infrastructure.Redis": {
        "ConnectionString": "127.0.0.1:6379"
    },
    "Application.ExchangeRate": {
        "SupportedPairs": [ "BTC/EURB", "ETH/EURB", "BINE/EURB" ],
        "ExchangeExcludePairs": [ "BINE/EURB" ],
        "BineRefreshRate": "1.00:0:00",
        "BineBaseValue": 0.1,
        "BineRange": [ 0.06, 0.12 ]
    }
}
